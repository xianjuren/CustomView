<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.custom.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/CustomViewTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.AutoLinefeedActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.NestClashActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.MoveScreenActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.SameDirectionConflictActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.DifferentDirectionConflictActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.MoveConflictActivity"
            android:screenOrientation="portrait"/>


        <activity
            android:name=".activity.SynthesizeConflictActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.RollingPictureActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>